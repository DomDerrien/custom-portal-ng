{
    "bail": false,
    "debug": false,
    "benchmark": false,
    "capabilities": {
        "browserName": "chrome",
        "goog:chromeOptions": {
            "args": [
                "headless",
                "disable-gpu",
                "disable-extensions"
            ],
            "mobileEmulation": {
                "deviceName": "Pixel 2"
            }
        }
    },
    "environments": [
        "node",
        {
            "browserName": "chrome",
            "fixSessionCapabilities": true
        }
    ],
    "functionalSuites": [
        "./dist/functional/**/*.js"
    ],
    "tunnel": "null",
    "tunnelOptions": {
        "drivers": [
            "chrome"
        ]
    },
    "leaveRemoteOpen": false
}
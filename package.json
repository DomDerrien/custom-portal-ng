{
    "name": "custom-portal-ng",
    "version": "1.0.0",
    "description": "This is version 2 of the Custom Portal Page, meant to be replacement of Google Bookmarks.",
    "author": "Dom Derrien <dominique.derrien@hgmail.com>",
    "dependencies": {
        "@google-cloud/datastore": "^1.3.4",
        "body-parser": "1.18.2",
        "express": "4.13.3",
        "morgan": "1.9.0",
        "node-fetch": "1.7.3",
        "nodemon": "1.14.12",
        "typescript": "2.6.2"
    },
    "devDependencies": {
        "@types/express": "4.11.1",
        "@types/google-cloud__datastore": "^1.3.0",
        "@types/node": "8.5.9",
        "@types/node-fetch": "1.6.7",
        "concurrently": "^3.5.1",
        "intern": "4.1.5",
        "sinon": "^4.4.2",
        "source-map-support": "0.5.3"
    },
    "scripts": {
        "start": "nodemon -r source-map-support/register server.js --watch dist/server",
        "watch": "concurrently --names \"server,client\" -c \"bgBlue.dim,black.bgGreen.dim\" \"tsc -p src/server/tsconfig.json --watch\" \"tsc -p src/client/tsconfig.json --watch\"",
        "test": "intern"
    },
    "engines": {
        "node": "8.x.x"
    },
    "type": "dist/d.ts"
}